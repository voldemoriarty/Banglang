
str FINDS THE ROOT OF AN EQUATION INEFFICIENTLY;
out_s

str X^2 + 5 = 66;
out_s

num 0
stloc_n 0

label WHILEBLC1
ldloc_n 0
ldloc_n 0
mul
num 5
add
num 66
sub
num 0.005
cl WEND1

ldloc_n 0
num 0.001
add
stloc_n 0
jmp WHILEBLC1
label WEND1

str ;
out_s

str AT X:;
out_s

ldloc_n 0
out_n

str X^2+5 is;
out_s

ldloc_n 0
ldloc_n 0
mul
num 5
add
out_n
